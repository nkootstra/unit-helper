{
    "name": "nkootstra/unit-conversion2",
    "description": "",
    "keywords": [
        "nkootstra",
        "unit-conversion2"
    ],
    "homepage": "https://github.com/nkootstra/unit-conversion2",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Niels Kootstra",
            "email": "niels.kootstra@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "chriskonnertz/string-calc": "^1.0",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Nkootstra\\UnitConversion\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nkootstra\\UnitConversion\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "UnitConversionServiceProvider"
            ],
            "aliases": {
                "UnitConversion2": "UnitConversionFacade"
            }
        }
    }
}
